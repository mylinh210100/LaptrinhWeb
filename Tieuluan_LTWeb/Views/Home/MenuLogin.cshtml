@{
    var sslogin = Session["login"] as Tieuluan_LTWeb.Models.SessionLogin;
    var ssregis = Session["register"] as Model.Model.TaiKhoan;
}

@if (sslogin == null && ssregis == null)
{
    <li class="nav-item">
        <a class="nav-link" href="/log-in">Log in</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" href="/register">Register</a>
    </li>

}
else if (sslogin != null)
{
    <li class="nav-item">
        <a class="nav-link" href="@Url.Action("Logout", "Login")">Log out</a>
    </li>
    <li class="nav-item">
        <a class="nav-link">Hello: @sslogin.name</a>
    </li>
}
else if (ssregis != null)
{
    <li class="nav-item">
        <a class="nav-link" href="@Url.Action("Logout", "Login")">Log out</a>
    </li>
    <li class="nav-item">
        <a class="nav-link">Hello: @ssregis.TenTaiKhoan</a>
    </li>

}